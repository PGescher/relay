{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler", 
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@relay/shared": ["../../packages/shared/src/notneededindex.ts"]
    }
  },
  "include": ["src/**/*.ts", "prisma.config.ts"]
}